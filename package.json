{
  "name": "rump-scripts-test",
  "version": "0.8.0",
  "author": "Juan Placencia",
  "description": "Rump piece for testing scripts with Karma",
  "license": "MIT",
  "homepage": "https://github.com/rumps/scripts-test",
  "main": "./lib",
  "repository": "rumps/scripts-test",
  "keywords": [
    "babel",
    "bower",
    "coffee-script",
    "gulp",
    "karma",
    "jasmine",
    "javascript",
    "mocha",
    "phantomjs",
    "qunit",
    "rump",
    "rump-module",
    "webpack"
  ],
  "scripts": {
    "codecov": "npm run coverage",
    "postcodecov": "codecov < coverage/coverage.json",
    "precompile": "[ -d lib ] && trash lib || true",
    "compile": "babel src --out-dir lib --source-maps=inline",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report json --report html --report text node_modules/mocha/bin/_mocha -- -R min",
    "lint": "eslint src test",
    "pub": "npm run test && npm run compile && npm publish",
    "pretest": "npm run lint",
    "test": "mocha"
  },
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "chalk": "^1.1.0",
    "extend": "^3.0.0",
    "globule": "^0.2.0",
    "karma": "^0.13.9",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "webpack": "^1.11.0"
  },
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-eslint": "^4.0.5",
    "bluebird": "^2.9.34",
    "codecov.io": "^0.1.5",
    "eslint": "^1.0.0",
    "gulp": "^3.9.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "rump": "^0.8.0",
    "rump-scripts": "^0.8.0",
    "should": "^7.0.2",
    "should-sinon": "0.0.3",
    "sinon": "^1.15.4",
    "timeout-then": "^1.0.0",
    "trash": "^2.0.0"
  }
}
